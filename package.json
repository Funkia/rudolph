{
  "name": "@funkia/rudolph",
  "version": "0.0.4",
  "description": "A pure functional router written in typescript",
  "main": "dist/router.js",
  "module": "dist/es2015/router.js",
  "typings": "dist/defs/router.d.ts",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "test": "karma start --single-run",
    "test-watch": "karma start",
    "codecov": "codecov -f coverage/**/json",
    "build": "npm run build-cmjs; npm run build-es2015",
    "build-es2015": "tsc -P ./tsconfig-release.json --outDir 'dist/es2015' --module es6 --moduleResolution node",
    "build-cmjs": "tsc -P ./tsconfig-release.json",
    "clean": "rm -rf coverage dist",
    "prepare": "npm run clean; npm run build",
    "release-major": "xyz --repo git@github.com:funkia/rudolph.git --increment major",
    "release-minor": "xyz --repo git@github.com:funkia/rudolph.git --increment minor",
    "release-patch": "xyz --repo git@github.com:funkia/rudolph.git --increment patch"
  },
  "author": "Funkia",
  "license": "MIT",
  "homepage": "https://github.com/funkia/rudolph#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funkia/rudolph.git"
  },
  "bugs": {
    "url": "https://github.com/funkia/rudolph/issues"
  },
  "keywords": [
    "pure",
    "functional",
    "router",
    "frp",
    "functional reactive programming",
    "typescript"
  ],
  "dependencies": {
    "@funkia/io": "0.0.2"
  },
  "peerDependencies": {
    "@funkia/hareactive": "0.1.3"
  },
  "devDependencies": {
    "@funkia/hareactive": "0.1.3",
    "@types/chai": "^4.0.8",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.1.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^3.0.8",
    "mocha": "^4.0.1",
    "sinon": "^4.1.3",
    "typescript": "^2.6.2",
    "xyz": "^2.1.0"
  }
}
